    public static final String EVENTPROCESS_DELETE_SUCCESSFUL = "eventprocess_delete_Successful";
    public static final String EVENTPROCESS_DEPLOYED_SUCCESSFUL = "eventprocess_deployed_Successful";
    public static final String EVENTPROCESS_STOP_SUCCESSFUL = "eventprocess_stop_Successful";
    public static final String SERVICEINSTANCE_LAUNCH_SUCCESSFUL = "serviceinstance_launch_Successful";
