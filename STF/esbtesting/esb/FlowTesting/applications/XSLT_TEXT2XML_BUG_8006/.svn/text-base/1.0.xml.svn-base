	value[&amp;quot;NNN&amp;quot;]=MONTH_NAMES[M+11];
	value[&amp;quot;d&amp;quot;]=d;
	value[&amp;quot;dd&amp;quot;]=LZ(d);
	value[&amp;quot;E&amp;quot;]=DAY_NAMES[E+7];
	value[&amp;quot;EE&amp;quot;]=DAY_NAMES[E];
